{
  "extends": "@tsconfig/react-native/tsconfig.json",
  "compilerOptions": {
    "baseUrl": "./",
    "paths": {
      // root
      "@src/*": ["./src/*"],
      // assets
      "@assets/*": ["./assets/*"],
      "@components/*": ["./src/components/*"],
      "@constants": ["./src/constants"],
      "@navigation/*": ["./src/navigation/*"],
      "@screens/*": ["./src/screens/*"]
    }
  }
}
